{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "userID": {
            "type": "integer"
        },
        "cardID": {
            "type": "string",
            "format": "uuid"
        },
        "foil": {
            "type": "string"
        },
        "forTrade": {
            "type": "boolean"
        },
        "quality": {
            "type": "string"
        },
        "quantity": {
            "type": "integer"
        }
    },
    "additionalProperties": false,
    "required": [
        "userID",
        "cardID",
        "foil",
        "forTrade",
        "quality",
        "quantity"
    ]
}